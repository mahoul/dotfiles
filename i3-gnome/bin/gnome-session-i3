#! /bin/sh

if [ -d $HOME/bin ]; then
	export $PATH=$HOME/bin:$PATH
fi

gnome-session --help 2>/dev/null | grep -- --builtin > /dev/null
HASBUILTIN=$?
if [ $HASBUILTIN -eq 0 ]; then
    BUILTINARG="--builtin"
else
    BUILTINARG=""
fi
env GNOME_SHELL_SESSION_MODE=classic gnome-session $BUILTINARG --session i3-gnome "$@"
